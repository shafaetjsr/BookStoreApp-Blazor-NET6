{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "./log/log-.txt",
          "rollingInterval": "Day"
        }
      }
    ] 
  },
  "AllowedHosts": "*"
}
